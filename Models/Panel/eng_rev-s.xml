<?xml version="1.0"?>
  <PropertyList> 
    <path>eng_rev.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>Oil-Pressure-Needle1</object-name>
      <property>engines/engine[1]/rpm</property>
      <interpolation>
        <entry> 
          <ind>0</ind>
          <dep>-28</dep>
        </entry> 
        <entry> 
          <ind>200</ind>
          <dep>-25</dep>
        </entry>
        <entry> 
          <ind>500</ind>
          <dep>-20</dep>
        </entry>
        <entry> 
          <ind>800</ind>
          <dep>-15</dep>
        </entry>
        <entry> 
          <ind>1100</ind>
          <dep>-10</dep>
        </entry>
        <entry> 
          <ind>1400</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1700</ind>
          <dep>5</dep>
        </entry>
        <entry> 
          <ind>2000</ind>
          <dep>10</dep>
        </entry>
        <entry> 
          <ind>2300</ind>
          <dep>15</dep>
        </entry>
		<entry> 
          <ind>2600</ind>
          <dep>20</dep>
        </entry>
		<entry> 
          <ind>30000</ind>
          <dep>25</dep>
        </entry>
       	<entry> 
          <ind>3000</ind>
          <dep>28</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>-0.065</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>0</x>
        <y>-1</y>
        <z>0</z>
      </axis>
    </animation>
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.75</green>
        <blue>0.75</blue>
      </emission>
      <shininess>10</shininess>
    </animation>
    <!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
