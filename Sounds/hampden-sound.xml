<?xml version="1.0" encoding="UTF-8"?>

<PropertyList> 

  <fx> 

    <crank> 
      <name>engstart</name>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_start.wav</path>
      <property>/engines/engine[0]/cranking</property>
      <volume>
        <offset>-0.4</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

<crank> 
      <name>engstart</name>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_start.wav</path>
      <property>/engines/engine[1]/cranking</property>
      <volume>
        <offset>-0.4</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine> 
      <name>engine idle rpm</name>
      <mode>looped</mode>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_run1.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
       <less-than> 
          <property>/engines/engine[0]/rpm</property>
          <value>1500</value>
        </less-than>
        <greater-than> 
          <property>/engines/engine[0]/rpm</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[0]/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

<engine> 
      <name>engine idle rpm</name>
      <mode>looped</mode>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_run1.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
       <less-than> 
          <property>/engines/engine[1]/rpm</property>
          <value>1500</value>
        </less-than>
        <greater-than> 
          <property>/engines/engine[1]/rpm</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[1]/rpm</property>
        <factor>0.0009496</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine> 
      <name>engine high rpm</name>
      <mode>looped</mode>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_run2.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
        <greater-than-equals> 
          <property>/engines/engine[0]/rpm</property>
          <value>1499</value>
        </greater-than-equals>
      </condition>
      <volume> 
       <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[0]/rpm</property>
        <factor>0.000462</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine> 
      <name>engine high rpm</name>
      <mode>looped</mode>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_run2.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
        <greater-than-equals> 
          <property>/engines/engine[1]/rpm</property>
          <value>1499</value>
        </greater-than-equals>
      </condition>
      <volume> 
       <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[1]/rpm</property>
        <factor>0.000462</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine> 
      <name>engine shut down rpm</name>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_shutdown.wav</path>
      <condition> 
        <property>/engines/engine[0]/running</property>
        <not> 
          <property>controls/engines/engine[0]/cutoff</property>
        </not>
        <less-than-equals> 
          <property>/engines/engine[0]/rpm</property>
          <value>150</value>
        </less-than-equals>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

<engine> 
      <name>engine shut down rpm</name>
      <path>Aircraft/hp-hampden-mki/Sounds/peg_shutdown.wav</path>
      <condition> 
        <property>/engines/engine[1]/running</property>
        <not> 
          <property>controls/engines/engine[1]/cutoff</property>
        </not>
        <less-than-equals> 
          <property>/engines/engine[1]/rpm</property>
          <value>150</value>
        </less-than-equals>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <rumble> 
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition> 
        <or> 
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
        <property>/sim/current-view/internal</property>
      </condition>
      <volume> 
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.25</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>

    <gear> 
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Aircraft/hp-hampden-mki/Sounds/gear.wav</path>
      <property>/gear/gear[0]/position-norm</property>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>
  
    <squeal> 
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition> 
        <or> 
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume> 
        <property>/velocities/speed-down-fps</property>
        <factor>0.5</factor>
        <max>1.0</max>
      </volume>
      <volume> 
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <max>1.0</max>
      </volume>
      <volume> 
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch> 
        <property>/velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>

    <flap> 
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/hp-hampden-mki/Sounds/flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume> 
        <factor>0.9</factor>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </flap>

    
    <click> 
      <name>rgs main click down</name>
      <path>Sounds/click.wav</path>
      <condition> 
        <property>controls/switches/gun-sight-main</property>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>

<click> 
      <name>rgs main click down</name>
      <path>Sounds/click.wav</path>
      <condition> 
        <property>controls/switches/gun-sight-main</property>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>

<click> 
      <name>rgs main click down</name>
      <path>Sounds/click.wav</path>
      <condition> 
        <property>controls/switches/gun-sight-main</property>
      </condition>
      <volume> 
        <factor>1.0</factor>
      </volume>
    </click>

  </fx>

</PropertyList>
